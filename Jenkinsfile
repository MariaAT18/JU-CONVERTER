pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World G1'
            }
        }
        stage('Build') {
            steps {
                echo 'Build G1'
            }
        }
        stage('Build Test 2') {
            steps {
                echo 'Build Test 2 G1'
            }
        }
    }
}